{
  "name": "atom-codeship",
  "main": "./lib/atom-codeship",
  "version": "0.1.0",
  "description": "Get notified about Codeship build failures without leaving Atom.",
  "keywords": ["codeship"],
  "repository": "https://github.com/atom/atom-codeship",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0",
    "node": ">=8.4.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint --ext .js ./",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --write", "git add"]
  },
  "atomTranspilers": [
    {
      "glob": "**/*.js",
      "transpiler": "atom-babel6-transpiler"
    }
  ],
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.3",
    "prettier-eslint-cli": "^4.2.0"
  },
  "dependencies": {
    "atom-babel6-transpiler": "^1.1.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-fetch": "^1.7.2",
    "opn": "^5.1.0"
  }
}
